{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "mythril_output.schema.json",
  "title": "DS Mythril Output",
  "description": "Wrapper schema for Mythril symbolic execution output on DS samples.",
  "type": "object",
  "required": ["sample_id", "tier", "tool", "tool_version", "timestamp", "success", "raw_output"],
  "properties": {
    "sample_id": {
      "type": "string",
      "pattern": "^ds_t[1-4]_\\d{3}$",
      "description": "Sample identifier (e.g., ds_t1_001)."
    },
    "tier": {
      "type": "integer",
      "minimum": 1,
      "maximum": 4,
      "description": "Difficulty tier (1-4)."
    },
    "tool": {
      "type": "string",
      "const": "mythril",
      "description": "Tool name."
    },
    "tool_version": {
      "type": "string",
      "description": "Mythril version (e.g., 0.24.8)."
    },
    "solc_version": {
      "type": "string",
      "description": "Solidity compiler version used (e.g., 0.8.0)."
    },
    "timestamp": {
      "type": "string",
      "format": "date-time",
      "description": "ISO 8601 timestamp of execution."
    },
    "success": {
      "type": "boolean",
      "description": "Whether Mythril executed successfully."
    },
    "error": {
      "type": ["string", "null"],
      "description": "Error message if execution failed, null otherwise."
    },
    "timeout": {
      "type": "boolean",
      "description": "Whether execution timed out."
    },
    "execution_time_ms": {
      "type": "number",
      "minimum": 0,
      "description": "Execution time in milliseconds."
    },
    "exit_code": {
      "type": "integer",
      "description": "Process exit code."
    },
    "analysis_mode": {
      "type": "string",
      "enum": ["quick", "standard", "deep"],
      "description": "Mythril analysis depth mode used."
    },
    "raw_output": {
      "type": "object",
      "description": "Raw Mythril JSON output as-is. Structure defined by Mythril.",
      "properties": {
        "success": { "type": "boolean" },
        "error": { "type": ["string", "null"] },
        "issues": {
          "type": "array",
          "description": "Array of detected issues with SWC IDs.",
          "items": {
            "type": "object",
            "properties": {
              "swc-id": { "type": "string" },
              "title": { "type": "string" },
              "description": { "type": "string" },
              "severity": { "type": "string" },
              "address": { "type": "integer" },
              "contract": { "type": "string" },
              "function": { "type": "string" },
              "sourceMap": { "type": "string" }
            }
          }
        }
      }
    }
  },
  "additionalProperties": false
}
